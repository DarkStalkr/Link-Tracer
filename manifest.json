{
    "manifest_version": 3,
    "version": "1.0",
    "name": "LinkTracer",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "webRequest",
        "proxy",
        "contextMenus"
        
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo2.png",
            "48": "logo2.png",
            "128": "logo2.png"
        }
    },
    "icons": {
        "48": "logo2.png",
        "128": "logo2.png"
    }
}
